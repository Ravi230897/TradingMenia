@model TradingMenia.Controllers.MarginsController

<h2>Get Margins</h2>

<form method="post">
    <div>
        <br />
        <label for="apiKey">API Key:</label>
        <input type="text" id="apiKey" name="apiKey" required />
        <br />
    </div>
    <div>
        <br />
        <label for="accessToken">Access Token:</label>
        <input type="text" id="accessToken" name="accessToken" required />
        <br />
    </div>
    <br />
    <button type="submit">Get Margins</button>
</form>

@if (ViewData.ContainsKey("Margins"))
{
    <h3 style="border:dashed; height:auto; width:auto">Margins: @ViewData["Margins"]</h3>
}

@if (ViewData.ContainsKey("Error"))
{
    <h3>Error: @ViewData["Error"]</h3>
}
